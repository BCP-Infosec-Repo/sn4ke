#!/home/pwnslinger/Projects/mutant/sn4ke/gtirb-capstone/venv/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'gtirb-functions==0.1.0','console_scripts','functions'
__requires__ = 'gtirb-functions==0.1.0'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('gtirb-functions==0.1.0', 'console_scripts', 'functions')()
    )
